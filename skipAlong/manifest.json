{

    "manifest_version": 2,
    "name": "skipAlong",
    "version": "1.0",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
  
    "icons": {
      "48": "icons/speedButton.png"
    },

    "permissions": [
        "activeTab"
    ],
  
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["skipAlong.js"]
      }
    ]
  
  }
  